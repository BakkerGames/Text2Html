  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <DebugType>embedded</DebugType>
    <DebugSymbols>true</DebugSymbols>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Resources\ebookstyle.css" />
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Include="Resources\ebookstyle.css" />
  </ItemGroup>

</Project>
